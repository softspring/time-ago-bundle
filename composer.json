{
    "name": "softspring/time-ago-bundle",
    "description": "This bundle provides a simple twig extension to human readable date diff based on translations",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.52.*",
        "phpstan/phpstan": "^1.10",
        "symfony/config": "^5.4|^6.0|^7.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0",
        "symfony/translation": "^5.4|^6.0|^7.0",
        "twig/twig": "*"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\TimeAgoBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
